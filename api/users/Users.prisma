import { UserExercises } from "../userexercises/UserExercises"
import { UserExerciseSets } from "../userexercises/userexercisesets/UserExerciseSets"
import { TemplateWorkouts} from "../workouts/templateworkouts/TemplateWorkouts"
import { ScheduledWorkouts } from "../workouts/scheduledworkouts/ScheduledWorkouts"
import { CompletedWorkouts } from "../workouts/completedworkouts/CompletedWorkouts"

model Users {
  profilePhoto String?
  bio String?
  firstName String?
  lastName String?
  salt String
  userId String       @id @unique @default(uuid())
  userName String     @unique
  email String        @unique
  password String     @db.VarChar(60)
  createdAt DateTime  @default(now()) @db.Timestamp(6)
  updatedAt DateTime  @updatedAt

  templateWorkouts TemplateWorkouts[]
  userExercises UserExercises[]
  userExerciseSets UserExerciseSets[]
  scheduledWorkouts ScheduledWorkouts[]
  completedWorkouts CompletedWorkouts[]
}