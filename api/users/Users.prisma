import { UserExercises } from "../userexercises/UserExercises"
import { UserExerciseSets } from "../userexercises/UserExerciseSets"
import { TemplateWorkouts} from "../workouts/data/templateworkouts/TemplateWorkouts"
import { ScheduledWorkouts } from "../workouts/data/scheduledworkouts/ScheduledWorkouts"
import { CompletedWorkouts } from "../workouts/data/completedworkouts/CompletedWorkouts"
import { WeeklySnapshots } from "../workouts/snapshots/WeeklySnapshots"

model Users {
  profilePhoto String?
  bio String?
  firstName String?
  lastName String?
  salt String
  userId String       @id @unique @default(uuid())
  userName String     @unique
  email String        @unique
  password String     @db.VarChar(60)
  createdAt DateTime  @default(now()) @db.Timestamp(6)
  updatedAt DateTime  @updatedAt

  templateWorkouts TemplateWorkouts[]
  userExercises UserExercises[]
  userExerciseSets UserExerciseSets[]
  scheduledWorkouts ScheduledWorkouts[]
  completedWorkouts CompletedWorkouts[]
  weeklySnapshots WeeklySnapshots[]
}