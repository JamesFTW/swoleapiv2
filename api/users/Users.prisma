
model Users {
  profilePhoto String?
  bio String?
  firstName String?
  lastName String?
  userId Int          @id @unique @default(autoincrement())
  userName String     @unique
  email String        @unique
  password String     @db.VarChar(60)
  createdAt DateTime  @default(now()) @db.Timestamp(6)
  updatedAt DateTime  @updatedAt
}