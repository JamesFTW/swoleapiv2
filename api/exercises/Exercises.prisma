import { UserExercises } from "../userexercises/UserExercises"
import { TemplateWorkoutExercises } from "../workouts/data/templateworkouts/templateworkoutexercises/TemplateWorkoutExercises"
import { CompletedWorkouts } from "../workouts/data/completedworkouts/CompletedWorkouts"

model Exercises {
  exerciseId Int @id @unique @default(autoincrement())
  createdAt DateTime  @default(now()) @db.Timestamp(6)
  updatedAt DateTime  @updatedAt
  exerciseName String @unique
  targetMuscle String
  secondaryMuscles Json?
  video String

  userExercises UserExercises[]
  templateworkoutexercises TemplateWorkoutExercises[]
  completedWorkouts CompletedWorkouts[]
}