import { Users } from "../../../users/Users"
import { UserExercises, UserExercises } from "../../../userexercises/UserExercises"

model CompletedWorkouts {
  completedWorkoutId Int @id @unique @default(autoincrement())
  userId String
  comment String?
  userExercises      UserExercises[] @relation
  userExerciseIds    Int[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  minHeartRate Int?
  maxHeartRate Int?
  duration Int
  workoutTitle String
  caloriesBurned Int?
  primaryMuscleGroup String
  totalVolume Int?

  User Users @relation(fields: [userId], references: [userId])
}