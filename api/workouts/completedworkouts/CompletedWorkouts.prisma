import { Users } from "../../users/Users"
import { UserExercises } from "../../userexercises/UserExercises"

model CompletedWorkouts {
  completedWorkoutId Int @id @unique @default(autoincrement())
  usedId String
  workoutId String
  comment String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  minHeartRate Int?
  maxHeartRate Int?
  duration Int
  workoutTitle String
  caloriesBurned Int
  primaryMuscleGroup String

  User Users @relation(fields: [usedId], references: [userId])
  UserExercises UserExercises @relation(fields: [workoutId], references: [workoutId])
}