{
  "name": "swoleapiv2",
  "version": "1.0.0",
  "description": "## GET SWOLE!",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "db:start": "pscale shell swoleproddb main --environment=prod",
    "db:seed": "npx prisma db seed",
    "db:start:dev": "pscale shell swoleproddb dev",
    "db:start:dev:docker": "docker build -t db . && docker run -d -p 3306:3306 --name db -e MYSQL_ALLOW_EMPTY_PASSWORD=true mysql && docker ps",
    "db:migrate": "npx prisma db push --schema=./api/schema.prisma",
    "start:dev": "nodemon index.ts",
    "prisma:import": "npx prisma-import --force",
    "postinstall": "prisma generate --schema=./api/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesFTW/swoleapiv2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesFTW/swoleapiv2/issues"
  },
  "homepage": "https://github.com/JamesFTW/swoleapiv2#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/passport-openidconnect": "^0.1.0",
    "babel-jest": "^29.6.1",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "import": {
      "output": "./api/schema.prisma",
      "schemas": "./api/**/{*,.*}/*.prisma",
      "exclude": [
        "./api/schema.prisma"
      ]
    }
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@types/cookie-parser": "^1.4.3",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^6.1.5",
    "mysql2": "^3.3.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prisma-import": "^1.0.4"
  }
}
