{
  "name": "swoleapiv2",
  "version": "1.0.0",
  "description": "## GET SWOLE!",
  "main": "index.js",
  "scripts": {
    "test": "npx dotenv -e .env.development jest",
    "db:seed": "npx prisma db seed",
    "db:migrate": "npx prisma migrate dev --name $npm_config_name --schema=./api/schema.prisma",
    "db:push": "npx prisma db push --schema=./api/schema.prisma",
    "db:seed:dev": "npx dotenv -e .env.development -- npx prisma db seed",
    "db:pull:dev": "npx dotenv -e .env.development -- npx prisma db pull --schema=./api/schema.prisma",
    "db:migrate:dev": "npx dotenv -e .env.development -- npx prisma migrate dev --name $npm_config_name --schema=./api/schema.prisma",
    "db:push:dev": "npx dotenv -e .env.development -- npx prisma db push --schema=./api/schema.prisma",
    "db:generate:dev": "npx dotenv -e .env.development -- npx prisma generate --schema=./api/schema.prisma",
    "start:dev": "./dev-server-sequence.sh",
    "dev:server": "NODE_ENV=development npx dotenv -e .env.development nodemon index.ts",
    "prisma:import": "npx prisma-import --force",
    "postinstall": "prisma generate --schema=./api/schema.prisma",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesFTW/swoleapiv2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesFTW/swoleapiv2/issues"
  },
  "homepage": "https://github.com/JamesFTW/swoleapiv2#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/passport-openidconnect": "^0.1.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.6.1",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "prisma": "^4.12.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "import": {
      "output": "./api/schema.prisma",
      "schemas": "./api/**/{*,.*}/*.prisma",
      "exclude": [
        "./api/schema.prisma"
      ]
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@prisma/client": "^4.12.0",
    "@types/cookie-parser": "^1.4.3",
    "bcrypt": "^5.1.0",
    "connect-pg-simple": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.4.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^6.1.5",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prisma-import": "^1.0.4"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
